<html>
    <head>
        <title>Media Menu</title>
        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.1/jquery.min.js"></script>

        <link href="/media/ppk/metroui/css/modern.css" rel="stylesheet">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link href="/media/ppk/metroui/css/modern-responsive.css" rel="stylesheet">
    </head>
    <body class="metrouicss">
        <center>
            <embed type="application/x-vlc-plugin" pluginspage="http://www.videolan.org"
            target="{{url}}"
            id="vlc">
            </embed>
            <object classid="clsid:9BE31822-FDAD-461B-AD51-BE1D1C159921" codebase="http://download.videolan.org/pub/videolan/vlc/last/win32/axvlc.cab"></object>
        </center>
        <script type="text/javascript">
            $('#vlc').attr('width', $(window).width() * 0.8)
            $('#vlc').attr('height', $(window).height() * 0.8)
        </script>
    </body>
</html>